.container{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
}
.WhySend{
  width: 100%;
}
.ImageUp{
  margin-left: 50rpx;
  float: left;
  align-self: flex-start;
}
.title{
  width:650rpx;
  margin-top: 50rpx;
  height: 50rpx;
  padding-left: 20rpx;
  margin-bottom: 20rpx;
  font-weight: 600;
  border-left: solid black 7rpx;
}
.commitRequest{
  margin-top: 30rpx;
  width: 650rpx;
  display: flex;
  justify-content: space-around;
}
.safePart{
  height: 200rpx;
}
.upPart{
  width:600rpx;
  height: 200rpx;
  margin-top: 30rpx;
  border:dashed #8a8a8a 3rpx;
  display:flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  color: #8a8a8a;
}

.midContain{
  display: flex;
  flex-direction: column;
}
.leftUp{
  display: flex;
  align-items: center;
}
.upInner{
  display: flex;
  justify-content: center;
  align-items: center;
}
.downInner{
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 25rpx;
  color: red;
}