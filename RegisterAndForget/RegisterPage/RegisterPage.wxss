.container{
  height: 70%;
  width: 95%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: start;
  box-sizing: border-box;
}
page{
  display: flex;
  justify-content: center;
  align-items: center;
  background-color:#ffffff;
  height: 100vh;
}
.inner{
  width: 100%;
  margin:5rpx;
}
.text{
    font-size: large;
    font-weight:800;
}
.protocolText{
  height: 72rpx;
  line-height: 72rpx;
  font-size: 25rpx;
  color: grey;
  display:inline-block;
}
.protocol{
    font-size: 25rpx;
    color:#2894FF;
    display:inline-block;
    margin:0px 0px 30rpx 0px;
}



